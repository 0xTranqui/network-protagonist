{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "strict": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "noEmit": true,
      "noImplicitReturns": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@/services": ["services/index.ts"],
        "@/lib": ["lib/index.ts"],
        "@/types": ["types/index.ts"],
      }
    },
    "exclude": ["node_modules"],
  }